@model GymHub.Models.Users
@{
    ViewData["Title"] = "User Details";
}

<h2>User Details</h2>

<dl class="row">
    <dt class="col-sm-2">Full Name</dt>
    <dd class="col-sm-10">@Model.FullName</dd>

    <dt class="col-sm-2">User Name</dt>
    <dd class="col-sm-10">@Model.UserName</dd>

    <dt class="col-sm-2">Email</dt>
    <dd class="col-sm-10">@Model.Email</dd>

    <dt class="col-sm-2">Phone</dt>
    <dd class="col-sm-10">@Model.PhoneNumber</dd>
    
</dl>

<a asp-action="Users" class="btn btn-secondary">Back</a>
